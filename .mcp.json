{
  "mcpServers": {
    "MCP_DOCKER": {
      "command": "docker",
      "args": ["mcp", "gateway", "run"],
      "type": "stdio"
    },
    "sec-edgar-mcp": {
      "command": "uvx",
      "args": ["sec-edgar-mcp"],
      "env": {
        "SEC_EDGAR_USER_AGENT": "Sunny Research (sunny@research.local)"
      }
    },
    "uk-ons": {
      "command": "npx",
      "args": ["-y", "uk_ons_mcp_server"]
    },
    "met-museum": {
      "command": "npx",
      "args": ["-y", "metmuseum-mcp"]
    },
    "artic-museum": {
      "command": "npx",
      "args": ["-y", "artic-mcp"]
    },
    "app-insight-mcp": {
      "command": "npx",
      "args": ["-y", "@jeromyfu/app-insight-mcp"]
    },
    "patents": {
      "command": "uvx",
      "args": ["patent-mcp-server"]
    },
    "companies-house": {
      "command": "npx",
      "args": ["-y", "companies-house-mcp-server"],
      "env": {
        "COMPANIES_HOUSE_API_KEY": "NEEDS_FREE_KEY"
      }
    },
    "fred-mcp-server": {
      "command": "npx",
      "args": ["-y", "fred-mcp-server"],
      "env": {
        "FRED_API_KEY": "NEEDS_FREE_KEY"
      }
    },
    "rijksmuseum": {
      "command": "npx",
      "args": ["-y", "mcp-server-rijksmuseum"],
      "env": {
        "RIJKSMUSEUM_API_KEY": "NEEDS_FREE_KEY"
      }
    },
    "smithsonian": {
      "command": "npx",
      "args": ["-y", "smithsonian-mcp"]
    },
    "va-museum": {
      "type": "url",
      "url": "https://mcp.vam.ac.uk/mcp/"
    }
  }
}
